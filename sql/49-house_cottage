alter table house add column cottage int(11) not null default 0;
insert into house (name, max, bath, tent, center, cabin, priority, x, y, cluster_id, cluster_order, inactive, disp_code, comment, resident, cat_abode, sq_foot, key_card, cottage) values ('RAM 1 Cottage', 1, '', '', '', '', 9, 375, 315, 4, 9, '', 'L', '', '', '', '', 'yes', 3);
update house set cottage = 1 where name = 'Ram 1A';
update house set cottage = 1 where name = 'Ram 1B';
update house set cottage = 2 where name = 'Ram 2A';
update house set cottage = 2 where name = 'Ram 2B';
update house set cottage = 2 where name = 'Ram 2C';

#!/bin/sh
mig=/Users/jonbjornstad/catalyst/projects/RetreatCenter/migration
cd $mig

echo loading
sqlite3 retreatcenter.db <mkpeople
sqlite3 retreatcenter.db <mkmisc
sqlite3 retreatcenter.db <mkprog
sqlite3 retreatcenter.db <mkrep
sqlite3 retreatcenter.db <mkusers
sqlite3 retreatcenter.db <mkmember
sqlite3 retreatcenter.db <mkreg

loadstr
loadaf

cd ../root/static/images
rm -f lo* lth* lb* po* pth* pb*

cd $mig
#loadprog f89
#loadreg  f89
loadprog f07
loadreg  f07
#loadpeople

cp retreatcenter.db ..

drop table if exists member;
create table member (
    id integer primary key,
    person_id integer,
    category text,
    date_general text,
    date_sponsor text,
    sponsor_nights integer,
    date_life text,
    free_prog_taken text,
    date_lapsed text,
    total_paid integer
);
insert into member values (
    1,
    12930,
    'Sponsor',
    '',
    '20080109',
    10,
    '',
    '',
    '',
    105
);
drop table if exists spons_hist;
create table spons_hist (
    id integer primary key,
    member_id integer,
    date_payment text,
    amount integer
);
insert into spons_hist values (1, 1, 20080101, 40);
insert into spons_hist values (2, 1, 20080108, 60);
insert into spons_hist values (3, 1, 20080109, 5);

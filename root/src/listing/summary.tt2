<h2>[% section %] Needs from [% start %] to [% end %]</h2>
<table cellpadding=5>
<tr>
<th align=left>Event</th>
<th>Start</th>
<th>End</th>
<th align=left>Place</th>
<th align=left>[% section %] Needs</th>
</tr>
[% FOREACH ev IN events %]
    [% sum = ev.summary %]
    <tr>
    <td valign=top><a href=/summary/view/[% ev.event_type %]/[% ev.summary_id %]>[% ev.name %]</a></td>
    <td valign=top>[% ev.sdate_obj.format('%a %b %e %q') %]</td>
    <td valign=top>[% ev.edate_obj.format('%a %b %e %q') %]</td>
    <td valign=top>[% ev.meeting_places %]</td>
    <td valign=top>[% IF section == 'Flower' %][% sum.leader_housing_tr %]<p class=p2>[% sum.flowers_tr %][% ELSIF section ==  'Field Staff Setup' %][% sum.leader_housing_tr %]<p class=p2>[% sum.signage_tr %]<p class=p2>[% sum.field_staff_setup_tr %][% ELSE %][% sum.sound_setup_tr %][% END %]</td>
    </tr>
[% END %]
</table>
